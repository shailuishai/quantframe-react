{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Quantframe",
    "version": "1.3.4"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "allowlist": {
      "all": false,
      "dialog": {
        "all": false,
        "ask": false,
        "confirm": false,
        "message": false,
        "open": true,
        "save": false
      },
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "create": true
      },
      "protocol": {
        "all": true,
        "asset": true,
        "assetScope": ["**"]
      },
      "notification": {
        "all": true
      },
      "path": {
        "all": true
      },
      "fs": {
        "writeFile": true,
        "exists": true,
        "readDir": true,
        "copyFile": true,
        "removeFile": true,
        "scope": ["$APPDATA/**", "$DESKTOP/**"]
      },
      "http": {
        "all": true,
        "scope": []
      }
    },
    "updater": {
      "active": false,
      "endpoints": ["https://api.quantframe.app/release/{{target}}/{{arch}}/{{current_version}}"],
      "dialog": false,
      "pubkey": "untrusted comment: minisign public key: RWRTY0IyzHBBoGKbGFvUx1/eIsX3r/3BgibHig3CvAGE26IVePsAAAACAAAAAAAAAEAAAAAABTIgIoGcOR+H3mR6L9osIhynyOhaNBgDMuD40NaW5FzaMl3WtK+pV3IpmOfcHmn6Un0ztMO6TA8uJ+AN2aSf5kbLUjKgufS9UGhEWxO6cNOWiKnghD2Z5GOFM5YXE34udRkAaWHF/OU="
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "dev.kenya.quantframe",
      "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "userAgent": "Quantframe",
        "resizable": true,
        "title": "Quantframe v1.3.4 - Pre-release By Kenya-DK",
        "width": 1455,
        "height": 912
      }
    ]
  }
}
